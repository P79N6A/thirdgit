  padding: 10px;
}

.add-todo-time {
  height: 100px;
  width: 100px;
  background-color: red;
  display: flex;
  align-items: center;
  justify-content: center;
}

.page-add-changebar {
  height: 100px;
  width: 100px;
  background-color: #666666
}