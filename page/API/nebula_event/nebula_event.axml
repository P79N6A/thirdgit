<view class="page">
  <view class="page-description">监听容器事件</view>
  <view class="page-section">
    <button type="primary" onTap="titleClick">监听点击title</button>
    <button type="primary" onTap="offtitleClick">取消监听点击title</button>
    <button type="primary" onTap="resume">监听appResume</button>
    <button type="primary" onTap="offresume">取消监听appResume事件</button>
  </view>
</view>